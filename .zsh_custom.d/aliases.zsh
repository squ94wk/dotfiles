alias k="kubectl"
alias tf="terraform"
alias gf="echo \"git fetch & prune\" && git fetch -p"
alias ff="echo \"fast forward\" && git merge --ff-only"
alias gff="gf && ff"
alias co="git checkout"
alias push="git push"
alias stat="git status"
alias rbm="echo \"git rebase master\" && git rebase master"
alias pop="git stash pop"
alias stash="git stash"
alias compose="docker-compose up -d"
alias decompose="docker-compose down -v"
alias recompose="docker-compose down -v && docker-compose up -d"